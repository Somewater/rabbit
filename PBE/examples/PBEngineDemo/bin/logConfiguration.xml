<logConfiguration>
   <appender name="Trace" type="PBLabs.Engine.Debug.Log4PBE.TraceAppender"/>
   <appender name="UI" type="PBLabs.Engine.Debug.Log4PBE.UIAppender"/>
   <appender name="Exception" type="PBLabs.Engine.Debug.Log4PBE.ExceptionAppender"/>
