<h1><a href='<%= ADMINS_PATH %>'>ADMINS</a></h1>
<table width='100%' border='1'>
	<tr width='100%'>
		<th>id</th>
		<th>login</th>
		<th>password</th>
		<th>permissions</th>
		<th>level_low</th>
		<th>level_high</th>
		<th>created_at</th>
		<th>updated_at</th>
	</tr>
	<% @users.each_with_index do |user, index| %>
		<tr width='100%'>
        	<td><a href="<%= ADMINS_PATH %>?show=<%= user.id %>"><%= user.id %></a></td>
        	<td><%= user.login %></td>
        	<td><%= user.password %></td>
        	<td><%= user.permissions %></td>
			<td><%= user.level_low %></td>
			<td><%= user.level_high %></td>
        	<td><%= user.created_at ? user.created_at.to_formatted_s(:short) : 'null' %></td>
        	<td><%= user.updated_at ? user.updated_at.to_formatted_s(:short) : 'null' %></td>
		</tr>
	<% end %>
</table>